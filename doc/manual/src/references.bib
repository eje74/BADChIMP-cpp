@article{aursjo_inclusion_2018,
	title = {On the inclusion of mass source terms in a single-relaxation-time lattice {Boltzmann} method},
	volume = {30},
	issn = {1070-6631, 1089-7666},
	url = {http://aip.scitation.org/doi/10.1063/1.5024641},
	doi = {10.1063/1.5024641},
	abstract = {We present a lattice Boltzmann algorithm for incorporating a mass source in a fluid flow system. The
	proposed mass source/sink term, included in the lattice Boltzmann equation, maintains the Galilean
	invariance and the accuracy of the overall method, while introducing a mass source/sink term in the
	fluid dynamical equations. The method can, for instance, be used to inject or withdraw fluid from any
	preferred lattice node in a system. This suggests that injection and withdrawal of fluid does not have to
	be introduced through cumbersome, and sometimes less accurate, boundary conditions. The method
	also suggests that, through a chosen equation of state relating mass density to pressure, the proposed
	mass source term will render it possible to set a preferred pressure at any lattice node in a system.
	We demonstrate how this model handles injection and withdrawal of a fluid. And we show how it
	can be used to incorporate pressure boundaries. The accuracy of the algorithm is identified through a
	Chapman-Enskog expansion of the model and supported by the numerical simulations},
	language = {en},
	number = {5},
	urldate = {2019-11-15},
	journal = {Physics of Fluids},
	author = {Aursjø, Olav and Jettestuen, Espen and Vinningland, Jan Ludvig and Hiorth, Aksel},
	month = may,
	year = {2018},
	keywords = {Lattice Boltzmann, My Publications},
	pages = {057104},
	file = {1.5024641.pdf:/home/AD.NORCERESEARCH.NO/esje/Dropbox/EspensMappe/Zotero/JournalPapers/1.5024641.pdf:application/pdf},
}


